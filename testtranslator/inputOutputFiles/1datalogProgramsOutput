CREATE OR REPLACE VIEW SELECTION_RULE AS SELECT *  FROM A;
CREATE OR REPLACE VIEW SELECTION_RULE AS SELECT *  FROM A FROM A WHERE a='a2';
CREATE OR REPLACE VIEW INNER_JOIN_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW INNER_JOIN_RULE AS SELECT ,aAA INNER JOIN ;
CREATE OR REPLACE VIEW INNER_JOIN_RULE AS SELECT ,aAA INNER JOIN A INNER JOIN  ON A.a;CREATE OR REPLACE VIEW INNER_JOIN_RULE AS SELECT ,aAA INNER JOIN A INNER JOIN  ON A.a;A INNER JOIN  ON A.a=;
CREATE OR REPLACE VIEW LEFT_JOIN_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW LEFT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  INNER JOIN ;
CREATE OR REPLACE VIEW LEFT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  INNER JOIN A LEFT JOIN  INNER JOIN  ON A.a;CREATE OR REPLACE VIEW LEFT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  INNER JOIN A LEFT JOIN  INNER JOIN  ON A.a;A LEFT JOIN  INNER JOIN  ON A.a=;
CREATE OR REPLACE VIEW RIGHT_JOIN_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW RIGHT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  RIGHT JOIN  INNER JOIN ;
CREATE OR REPLACE VIEW RIGHT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  RIGHT JOIN  INNER JOIN A LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a;CREATE OR REPLACE VIEW RIGHT_JOIN_RULE AS SELECT ,aAA LEFT JOIN  RIGHT JOIN  INNER JOIN A LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a;A LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a=;
CREATE OR REPLACE VIEW FULL_JOIN_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW FULL_JOIN_RULE AS SELECT ,aAA FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN ;
CREATE OR REPLACE VIEW FULL_JOIN_RULE AS SELECT ,aAA FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN A FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a;CREATE OR REPLACE VIEW FULL_JOIN_RULE AS SELECT ,aAA FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN A FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a;A FULL JOIN  LEFT JOIN  RIGHT JOIN  INNER JOIN  ON A.a=;
CREATE OR REPLACE VIEW UNION_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW UNION_RULE AS SELECT ,aAA UNION SELECT ;
CREATE OR REPLACE VIEW UNION_RULE AS SELECT ,aAA UNION SELECT A UNION SELECT ;
CREATE OR REPLACE VIEW DIFFERENCE_RULE AS SELECT ,aA;
CREATE OR REPLACE VIEW DIFFERENCE_RULE AS SELECT ,aAA UNION SELECT ;
CREATE OR REPLACE VIEW DIFFERENCE_RULE AS SELECT ,aAA UNION SELECT A UNION SELECT ;
