CREATE OR REPLACE VIEW PROJECTION_RULE AS SELECT *  FROM A;
CREATE OR REPLACE VIEW SELECTION_RULE AS SELECT *  FROM A WHERE a='a2';
CREATE OR REPLACE VIEW INNER_JOIN_RULE AS SELECT b FROM ij FROM B WHERE a=b);
CREATE OR REPLACE VIEW LEFT_JOIN_RULE AS SELECT *  FROM lj FROM B WHERE a=b);
CREATE OR REPLACE VIEW RIGHT_JOIN_RULE AS SELECT *  FROM rj FROM B WHERE a=b);
CREATE OR REPLACE VIEW FULL_JOIN_RULE AS SELECT *  FROM fj FROM B WHERE a=b);
CREATE OR REPLACE VIEW UNION_RULE AS SELECT * *  FROM A UNION SELECT  FROM B;
CREATE OR REPLACE VIEW DIFFERENCE_RULE AS SELECT * b FROM A EXCEPT SELECT  FROM B;
